  "repository": {
    "type": "git",
    "url": "https://github.com/dilly97/mypod"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production DEBUG=app node dist/bundle.js",
    "dev": "NODE_ENV=development DEBUG=app nodemon --watch src src/index.js",
    "dev:build": "WEBPACK_ENV=development webpack -w",
